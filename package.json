{
  "name": "aws-cdk-building-block",
  "version": "0.0.1",
  "description": "AWS CDK Building Blocks",
  "author": "Leone Perdig√£o",
  "license": "Apache License 2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts {src,__tests__}/**/*.ts --color",
    "lint:fix": "npm run lint -- --fix",
    "clean": "find ./src -name \"*.d.ts\" -o -name \"*.js\" -o -name \"*.js.map\" | xargs rm && rm -R lib",
    "build": "npm run lint && tsc",
    "test": "jest",
    "test:badges": "npm run test && jest-coverage-badges --input ./coverage/coverage-summary.json --output ./__tests__/.badges"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leoneperdigao/aws-cdk-building-blocks"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.180.0",
    "@aws-cdk/aws-sam": "1.180.0",
    "@types/jest": "26.0.15",
    "@types/node": "14.11.8",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-typescript": "11.0.0",
    "eslint-config-google": "0.14.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-coverage-badges": "^1.0.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.0.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.180.0",
    "@aws-cdk/aws-cloudwatch": "1.180.0",
    "@aws-cdk/aws-ec2": "1.180.0",
    "@aws-cdk/aws-events": "1.180.0",
    "@aws-cdk/aws-iam": "1.180.0",
    "@aws-cdk/aws-kms": "1.180.0",
    "@aws-cdk/aws-lambda": "1.180.0",
    "@aws-cdk/aws-logs": "1.180.0",
    "@aws-cdk/aws-sns": "1.180.0",
    "@aws-cdk/aws-sqs": "1.180.0",
    "@aws-cdk/aws-ssm": "1.180.0",
    "@aws-cdk/core": "1.180.0",
    "@types/js-yaml": "3.12.5",
    "aws-cdk": "1.74.0",
    "js-yaml": "3.14.0",
    "lint-staged": "10.4.0",
    "source-map-support": "0.5.19"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "npm run lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npx lint-staged && npm run test"
    }
  }
}
